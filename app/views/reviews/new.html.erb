<h1>New Review on <%= (Restaurant.find(params[:restaurant_id]).name) %></h1>
<%# raise %>
<div class="container">
  <%= simple_form_for [@restaurant, @review] do |f| %>
    <%= f.input :rating, collection: [1, 2, 3, 4, 5] %>
    <%= f.input :content %>
    <%= f.submit "Add", class: "btn btn-primary" %>
  <% end %>
</div>
